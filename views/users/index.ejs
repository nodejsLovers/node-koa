<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>

</head>
<body>
<h1><%= title %></h1>
<ul id="users">
    <% if (users.length) { %>
        <% users.forEach(function(user){ %>
        <li><%= user.name %></li>
        <% }) %>
    <% } %>
</ul>
</body>
</html>
